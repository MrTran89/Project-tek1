#include "main.h"

int		my_form(t_room *r)
{
  unsigned int	d;
  
  r = malloc(sizeof(t_room));
  d = sqrt(((r->x - r->x->next) * (r->x + r->x->next)) + 
	   ((r->y - r->y->next) * (r->y - r->y->next)));
  return (d);
}

